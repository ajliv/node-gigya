{
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "comma-dangle": 0,
    "indent": ["warn", 4],
    "no-alert": 0,
    "no-console": 1,
    "no-extra-semi": 1,
    "no-trailing-spaces": [1, { "skipBlankLines": true }],
    "no-undef": 1,
    "no-underscore-dangle": 0,
    "no-unreachable": 1,
    "no-unused-vars": 1
  }
}
